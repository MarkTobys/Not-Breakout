<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>Not_Breakout</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="assets\ball.png" />
    <None Remove="assets\floor-tile.png" />
    <None Remove="assets\lava-tile.png" />
    <None Remove="assets\paddle.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="assets\ball.png" />
    <Resource Include="assets\floor-tile.png" />
    <Resource Include="assets\lava-tile.png" />
    <Resource Include="assets\paddle.png" />
  </ItemGroup>

</Project>
